export PORT=8080
ns-source default "$NETNS_SRC_BASE/start-proxy"

timeout 10 nc -N -l 9999 > $NETNS_PREFIX/out < /dev/null &
pid=$!
until lsof -i :9999 > /dev/null; do sleep .1; done
timeout 10 nc -N localhost $PORT > /dev/null < <(echo -en '\x05\x02\x01\x00\x05\x01\x00\x03\x09\x6c\x6f\x63\x61\x6c\x68\x6f\x73\x74\x27\x0f\x71\x61\x7a\x77\x73\x78\x65\x64\x63\x72\x66\x76\x74\x67\x62\x79\x68\x6e\x75\x6a\x6d\x69\x6b\x2c\x6f\x6c\x2e\x70\x3b\x2f\x5b'; sleep .5)
wait $pid
diff $NETNS_PREFIX/out <(echo -en '\x71\x61\x7a\x77\x73\x78\x65\x64\x63\x72\x66\x76\x74\x67\x62\x79\x68\x6e\x75\x6a\x6d\x69\x6b\x2c\x6f\x6c\x2e\x70\x3b\x2f\x5b')
